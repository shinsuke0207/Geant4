/gps/particle mu-

/gps/pos/type Surface
/gps/pos/shape Sphere
/gps/pos/centre 0 0 5 cm
/gps/pos/radius 7 cm

#/gps/pos/type Plane
#/gps/pos/shape Square
#/gps/pos/centre 0 0 1 cm
#/gps/pos/halfx 5 cm
#/gps/pos/halfy 5 cm

/gps/ang/type user
/gps/hist/type theta

# 天頂角分布の強度のデータを入れる
/control/execute muangle.mac

/gps/ene/type Arb
/gps/ene/diffspec 0
/gps/hist/type arb

# 天頂角分布のエネルギーのデータを入れる
/control/execute muenergy.mac

# 試しに自分で適当な値を入力してみる
#/gps/hist/point 1000.9231 0
#/gps/hist/point 1000.9231 0.3136
#/gps/hist/point 2000.1444 0.389
#/gps/hist/point 4000.4444 0.37899
#/gps/hist/point 8000.7441 0.0003805

/gps/hist/inter Lin
/tracking/verbose 1 
/run/beamOn 10